<b><%= name %></b><br>
<%= total %><%= value.total.toLocaleString() %><%= unit %><br>
<% value.slices.forEach(function (slice) { %>
<svg class="nlga_map-legend__color"><rect width="100%" height="100%" style="fill:<%= slice.color%>"></rect></svg><%= slice.label %>: <%= slice.value.toLocaleString() %><%= unit %> (<%= slice.percent.toLocaleString() %>%)<br>
<% }); %>